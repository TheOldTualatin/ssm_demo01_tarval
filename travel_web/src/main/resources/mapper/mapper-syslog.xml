<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yqx.ssm.dao.ISysLogDao">
    <insert id="save">
        insert into ssm_test.syslog values (#{id}, #{visitTime}, #{username}, #{ip}, #{url}, #{executionTime}, #{method})
    </insert>
</mapper>